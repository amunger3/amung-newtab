{
    "description": "Custom page loaded on new tab.",
    "manifest_version": 2,
    "name": "amung-newtab",
    "author": "Alex Munger",
    "version": "0.0.3",
    "homepage_url": "https://github.com/amunger3/amung-newtab",
    "icons": {
        "48": "img/uicon.svg",
        "64": "img/uicon.svg",
        "96": "img/uicon.svg"
    },
    "background": {
        "scripts": ["js/background.js"]
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "amung@gatech.edu"
        }
    },
    "chrome_url_overrides": {
        "newtab": "amung.html"
    },
    "browser_action": {
        "default_icon": "img/uicon.svg"
    }
}